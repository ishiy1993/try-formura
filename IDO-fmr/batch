docker run -it --rm -v $PWD:/work -u $UID:$GID ishiy1993/formura-full ./run ${TARGET} 0.6
docker run -it --rm -v $PWD:/work -u $UID:$GID ishiy1993/formura-full ./run ${TARGET} 0.7
docker run -it --rm -v $PWD:/work -u $UID:$GID ishiy1993/formura-full ./run ${TARGET} 0.8
docker run -it --rm -v $PWD:/work -u $UID:$GID ishiy1993/formura-full ./run ${TARGET} 0.9
docker run -it --rm -v $PWD:/work -u $UID:$GID ishiy1993/formura-full ./run ${TARGET} 1.0
docker run -it --rm -v $PWD:/work -u $UID:$GID ishiy1993/formura-full ./run ${TARGET} -0.1
